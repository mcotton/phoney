    $(document).ready(function() {

            var target = $('#phone_list');
            $.get('/api/v1/phone/search/<%= id %>', function(data) {
                _.forEach(data.rows, function(item) {
                    //console.log(item);
                    //if(item.key[0] === '<%= user.twilio_sid %>') {
                        target.append('<tr class=>'
                                                   + '<td>' + item.value + '</td>'  
                                                   + '</tr>');
               
                //self.parent().append('<tr><td><textarea></textarea><br><input type=button value=reply></tr>');
                        
                        //}
                });
            });
            
            
    });
    
    
    
    </script>